<apex:page showHeader="false">
    <head>
        <title>We're sorry</title>
       <meta charset="utf-8" />
       <meta name="viewport" content="width=device-width, initial-scale=1.0" />
       
       <style>
        .logo {
              margin-top: 5px;
              max-width: 200px;
              max-height: 60px;
              margin-left: 15px;
        }
        .headerSurvey {
          /*padding: 15px 30px 10px 0px;*/
          color: #1C4B98;
          font-size: 2.0rem;
          font-family: sans-serif;
