<apex:component controller="ICF_viewSurveyResultsComponentController" >
    <apex:attribute name="pSurveyId" description="The survey to use" 
                  type="Id" required="required" assignTo="{!surveyId}"/>
    <apex:form >
        <apex:outputPanel style="width:100%">
        <apex:commandButton style="float:right" onclick="window.open('{!results}');" value="{!$Label.LABS_SF_View_Results_In_New_Window}" /> 
        <br />   
        <br />
        <br />   
      </apex:outputPanel>
    </apex:form>

    <!-- results -->
    <apex:iframe src="{! '/apex/ICF_SurveyReportResults?rId=' + reportNoParamsId + '&sId=' + surveyId +'&reportLinkId=' + reportId }" scrolling="true"/>
   
</apex:component>